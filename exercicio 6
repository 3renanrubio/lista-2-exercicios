#include <stdio.h>
int main () {

float num 1, num 2. num 3;

printf("digite um numero separado em espaços:");
scanf ("%f %f %f", &num 1, &num 2, &num 3);

float menor = 1;
if (num2 < menor) {
menor = num2;

}

if (num3 < menor) {
menor = num3;

}

printf ("o menor numero é: %2f\n", menor);
}

return 0;

}
