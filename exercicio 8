#include <stdio.h>
int main () {

int fibonacci (int n) {
int a = 0;
int b = 1;
int temp;

if(n == 0)
return a;
else if (n == 1)
retun b;

for (int i = 2; i <= n; i ++) {
temp = a+b;
a = b;
b = temp;

}

return b;
}

int main () {
int n;

printf ("digite o valor de N para calcular o N-esimo termo de fibonacci");
scabf ("%d", &n);

int resultado = fibonacci (n);
printf ("o %d-esimo termo de fibonacci -e %d\n", resultado);
}

return 0;

}
